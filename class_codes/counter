#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x62ce0c3d6490 .scope module, "tb_counter" "tb_counter" 2 1;
 .timescale 0 0;
v0x62ce0c4228c0_0 .var "clk", 0 0;
v0x62ce0c422990_0 .net "out", 3 0, v0x62ce0c4226c0_0;  1 drivers
v0x62ce0c422a60_0 .var "rstn", 0 0;
S_0x62ce0c3d6620 .scope module, "c0" "counter" 2 7, 3 1 0, S_0x62ce0c3d6490;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rstn";
    .port_info 2 /OUTPUT 4 "out";
v0x62ce0c3d6850_0 .net "clk", 0 0, v0x62ce0c4228c0_0;  1 drivers
v0x62ce0c4226c0_0 .var "out", 3 0;
v0x62ce0c4227a0_0 .net "rstn", 0 0, v0x62ce0c422a60_0;  1 drivers
E_0x62ce0c4118c0 .event posedge, v0x62ce0c3d6850_0;
    .scope S_0x62ce0c3d6620;
T_0 ;
    %wait E_0x62ce0c4118c0;
    %load/vec4 v0x62ce0c4227a0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x62ce0c4226c0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x62ce0c4226c0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x62ce0c4226c0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x62ce0c3d6490;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x62ce0c4228c0_0;
    %inv;
    %store/vec4 v0x62ce0c4228c0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x62ce0c3d6490;
T_2 ;
    %vpi_call 2 17 "$monitor", "[%0tns] clk=%0b rstn=%0b out=0x%0h", $time, v0x62ce0c4228c0_0, v0x62ce0c422a60_0, v0x62ce0c422990_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x62ce0c4228c0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x62ce0c422a60_0, 0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x62ce0c422a60_0, 0;
    %delay 80, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x62ce0c422a60_0, 0;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x62ce0c422a60_0, 0;
    %delay 200, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x62ce0c3d6490;
T_3 ;
    %vpi_call 2 34 "$dumpvars" {0 0 0};
    %vpi_call 2 35 "$dumpfile", "counter.vcd" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
